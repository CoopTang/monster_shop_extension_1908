<%= form_tag users_path do %>
  <p>Name: <%= text_field_tag :name, @user.name %></p>
  <p>Address: <%= text_field_tag :address, @address.address %></p>
  <p>City: <%= text_field_tag :city, @address.city %></p>
  <p>State: <%= text_field_tag :state, @address.state %></p>
  <p>Zip: <%= text_field_tag :zip, @address.zip %></p>
  <p>Email: <%= text_field_tag :email %></p>
  <%= render partial: 'partials/user_password_template' %>
  <p><%= submit_tag "Create User" %></p>
<%end %>
